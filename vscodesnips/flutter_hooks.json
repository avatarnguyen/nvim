{
  "HookWidget": {
    "scope": "dart",
    "prefix": "hookW",
    "description": "Creates a HookWidget class.",
    "body": [
        "class $1 extends HookWidget {",
        "\tconst $1({Key? key}) : super(key: key);",
        "\n\t@override",
        "\tWidget build(BuildContext context) {",
        "\t\treturn const SizedBox();",
        "\t}",
        "}"
    ],
  },
  "StatefulHookWidget": {
    "scope": "dart",
    "prefix": "hookSW",
